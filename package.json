{
  "name": "serverless-playground",
  "version": "1.0.1",
  "main": "src/handler.js",
  "license": "MIT",
  "scripts": {
    "deploy:env": "sls deploy --stage $NODE_ENV",
    "deploy": "export NODE_ENV=dev && yarn deploy:env",
    "deploy:production": "export NODE_ENV=production && yarn deploy:env",
    "deploy:stage": "export NODE_ENV=stage && yarn deploy:env",
    "destroy": "sls remove",
    "lint": "tslint -c tslint.json 'src/**/*{.ts,.tsx}'",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged",
    "serve": "serverless offline start --host=0.0.0.0",
    "tail:getPropertyAvailability": "serverless logs --function getPropertyAvailability --tail",
    "test": "jest",
    "test:unit": "RUNNING_UNIT_TESTS=true jest --testPathIgnorePatterns='integration'",
    "test:unit:watch": "yarn test:unit --watch",
    "test:integration": "jest --testMatch='**/?(*.)+(integration).(spec|test).[jt]s?(x)'",
    "test:integration:watch": "yarn test:integration --watch",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:swagger": "swagger-tools validate swagger.yml",
    "build": "serverless webpack",
    "getPropertyAvailability": "serverless invoke local --function getPropertyAvailability --path fixtures/get-property-availability.json",
    "watch:getPropertyAvailability": "serverless invoke local --watch --function getPropertyAvailability --path fixtures/get-property-availability.json",
    "watch:warm": "serverless invoke local --watch --function getPropertyAvailability --path fixtures/warm.json",
    "touch": "find src -exec touch {} \\;"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "husky": "^1.0.0",
    "jest": "^24.3.0",
    "lint-staged": "^8.0.0",
    "prettier": "^1.16.4",
    "serverless": "^1.32.0",
    "serverless-offline": "^3.25.10",
    "serverless-webpack": "^5.2.0",
    "swagger-tools": "^0.10.4",
    "ts-jest": "^24.0.0",
    "ts-lint": "^4.5.1",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "webpack": "^4.17.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/bunyan": "^1.8.5",
    "@types/jest": "^24.0.9",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "@types/uuid": "^3.4.4",
    "ajv": "^6.12.3",
    "awesome-typescript-loader": "^5.2.1",
    "bunyan": "^1.8.12",
    "jsonwebtoken": "^8.5.0",
    "jwk-to-pem": "^2.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "typescript": "^3.2.4",
    "uuid": "^3.3.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix -c tslint.json 'src/**/*.{ts,tsx}'",
      "prettier --write",
      "git add"
    ]
  }
}
